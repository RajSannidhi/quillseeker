<div class="poet-box col-10 offset-1">
  <h1><%= @poet.name %></h1>
  <i><%= @poet.dates %></i>
  <br/><br/>
  <p><%= @poet.description %></p>
    <div class="float-right">
    <%= link_to 'Edit', edit_poet_path(@poet), class: 'btn btn-primary' %>
    <%= link_to 'Destroy', poet_path(@poet), method: :delete, data: {confirm: 'Are you sure you want to delete this?'},class: 'btn btn-danger' %>
  </div>
</div>